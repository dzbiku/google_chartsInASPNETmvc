@model WebApplication1.Models.HistoryModel
@{
    ViewBag.Title = "History Model in use";
}
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
        google.charts.load('current', { 'packages': ['corechart'] });

        //Call function after Google Chart is loaded, it is required, otherwise you may get error
        google.charts.setOnLoadCallback(DrawonLoad);

        function DrawonLoad() {
            $(function () {
                $.ajax({
                    type: 'GET',
                    url: '/Chart/GetChartHistory',
                    success: function (chartsdata) {
                        // Callback that creates and populates a data table,
                        // instantiates the pie chart, passes in the data and
                        // draws it.

                        //get JSONList from Object
                        var Data = chartsdata.JSONList;
                        var data = new google.visualization.DataTable();
                        console.log(Data);
                        data.addColumn('string', 'Data');
                        data.addColumn('number', 'PośrednioProdukcyjne');
                        data.addColumn('number', 'Nieobecności');

                        //Loop through each list data
                        for (var i = 0; i < Data.lenght; i++) {
                            data.addRow([Data[i].Rok, Data[i].PośrednioProdukcyjne, Data[i].Nieobecności]);
                        }

                        var options = {
            title: 'S-70l Carry Forward Labor Hours By Position (PZL) as of @DateTime.Now',
            hAxis: { title: 'Data', titleTextStyle: { color: 'red' } },
            bar: { groupWidth: '75%' },
            isStacked: true
        };


                        // Instantiate and draw our chart, passing in some options
                        var chart = new google.visualization.PieChart(document.getElementById('chart_div'));

                        //Draw pie chart command with data and chart options
                        chart.draw(data,options);

                    },
                    error: function () {
                        alert("Error loading data! Please try again.");
                    }
                });
            })

        }
</script>
<div id="chart_div" style="width: 700px; height: 400px;">
</div>