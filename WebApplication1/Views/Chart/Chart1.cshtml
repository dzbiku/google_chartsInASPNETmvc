@model WebApplication1.Models.TradingModel
@{
    ViewBag.Title = " Google Column Chart In an Asp.Net MVC Application";
}
<script type="text/javascript" src="https://www.google.com/jsapi"></script>
<script type="text/javascript">

    google.load("visualization", "1", { packages: ["corechart"] });
    google.setOnLoadCallback(drawChart);

    function drawChart() {
        // Create and populate the data table.
        var years = [@Model.ProductData.Year];
        var sales = [@Model.ProductData.Imports];
        var Purchase = [@Model.ProductData.Exports];

        var data = new google.visualization.DataTable();
        @*data.addColumn('string', '@Model.YearTitle');
        data.addColumn('number', '@Model.ImportsTitle');
        data.addColumn('number', '@Model.ExportsTitle');*@
        for (i = 0; i < years.length; i++) {
            data.addRow([years[i].toString(), sales[i], Purchase[i]]);
        }
        var options = {
            title: 'S-70l Carry Forward Labor Hours By Position (PZL) as of @DateTime.Now',
            hAxis: { title: '@Model.YearTitle', titleTextStyle: { color: 'red' } },
            bar: { groupWidth: '75%' },
            isStacked: true
        };

        var chart = new google.visualization.ColumnChart(document.getElementById('chart_div'));
        chart.draw(data, options);
    }
</script>
<div id="chart_div" style="width: 700px; height: 400px;">
</div>